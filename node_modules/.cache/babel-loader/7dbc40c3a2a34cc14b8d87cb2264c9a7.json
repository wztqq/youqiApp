A,MAAA;AACA;AAlIA;AA7CA,CAAA","sourcesContent":["<template>\r\n  <div style=\"width: 100%; height: 100%\" :id=\"id\"></div>\r\n</template>\r\n<script>\r\nimport echarts from \"echarts\";\r\nexport default {\r\n  name: \"barLineEchart\",\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      default: \"chart\",\r\n    },\r\n    optionObj: {\r\n      type: Object,\r\n      default: () => {},\r\n    },\r\n    /*\r\n      optionObj:{\r\n        title:'',\r\n        legendData:['总面积'], //legend.data以及series.name\r\n        seriesName:\"\"\r\n        seriesData:[], \r\n      }\r\n    */\r\n  },\r\n  data() {\r\n    return {\r\n      chartBar: null,\r\n    };\r\n  },\r\n  created: function () {\r\n    this.$nextTick(function () {\r\n      this.getBarOption();\r\n    });\r\n  },\r\n  watch: {\r\n    id() {\r\n      this.$nextTick(function () {\r\n        this.getBarOption();\r\n      });\r\n    },\r\n    optionObj: {\r\n      handler: function (newVal, oldVal) {\r\n        if (newVal) {\r\n          this.getBarOption();\r\n        }\r\n      },\r\n      deep: true,\r\n    },\r\n  },\r\n  methods: {\r\n    getBarOption() {\r\n      this.chartBar = null;\r\n      this.chartBar = echarts.init(document.getElementById(this.id));\r\n      let option = {\r\n        tooltip: {\r\n          trigger: \"item\",\r\n          formatter: \"{a} <br/>{b}: {c} ({d}%)\"\r\n        },\r\n        // title: {\r\n        //   zlevel: 0,\r\n        //   text: [\"{value|\" + this.optionObj.total + \"}\", \"{name|亿m³}\"].join(\"\\n\"),\r\n        //   x: \"center\",\r\n        //   y: \"center\",\r\n        //   // textAlign: \"center\",\r\n        //   textStyle: {\r\n        //     rich: {\r\n        //       value: {\r\n        //         color: \"#4D607C\",\r\n        //         fontSize: 24,\r\n        //         fontWeight: \"bold\",\r\n        //         lineHeight: 36,\r\n        //       },\r\n        //       name: {\r\n        //         color: \"#4D607C\",\r\n        //         lineHeight: 22,\r\n        //         fontSize: 18,\r\n        //         fontWeight: \"bold\",\r\n        //       },\r\n        //     },\r\n        //   },\r\n        // },\r\n        legend: {\r\n          itemWidth: 10,\r\n          itemHeight: 6,\r\n          textStyle: {\r\n            color: \"#9C9DA2\",\r\n          },\r\n          x: \"center\",\r\n          top: '75%',\r\n          data: this.optionObj.legendData,\r\n        },\r\n        grid: {\r\n          top: '3%',\r\n          bottom: '5%'\r\n        },\r\n        series: [\r\n          {\r\n            name: this.optionObj.seriesName,\r\n            type: \"pie\",\r\n            radius: [\"35%\", \"50%\"],\r\n            center: [\"50%\", \"40%\"],\r\n            labelLine: {\r\n              normal: {\r\n                length: 10,\r\n                length2: 80,\r\n              },\r\n            },\r\n            label: {\r\n              normal: {\r\n                show: true,\r\n                color: \"#9C9DA2\",\r\n                fontSize: 14,\r\n                formatter: \"{d}%\",\r\n                rich: {\r\n                  c: {\r\n                    color: \"#9C9DA2\",\r\n                    fontWeight: \"bold\",\r\n                    fontSize: 40,\r\n                    align: \"center\",\r\n                    verticalAlign: \"bottom\",\r\n                  },\r\n                },\r\n                padding: [0, -50, 25, -50],\r\n              },\r\n            },\r\n\r\n            data: this.optionObj.seriesData,\r\n            itemStyle: {\r\n              emphasis: {\r\n                shadowBlur: 10,\r\n                shadowOffsetX: 0,\r\n                shadowColor: \"rgba(0, 0, 0, 0.5)\",\r\n              },\r\n              normal: {\r\n                color: function (params) {\r\n                  var colorList = [\r\n                    {\r\n                      c1: \" #11D59C\", \r\n                      c2: \"#24F6B2\",\r\n                    },\r\n                    {\r\n                      c1: \" #FF860D\",\r\n                      c2: \"#FFA516\",\r\n                    },\r\n                    {\r\n                      c1: \"#969BF2\",\r\n                      c2: \"#31D9E5\",\r\n                    },\r\n                    {\r\n                      c1: \"#FF10B9\",\r\n                      c2: \"#D05FF0\",\r\n                    },\r\n                    {\r\n                      c1: \"#EB5539\",\r\n                      c2: \"#F0765F\",\r\n                    },\r\n                    {\r\n                      c1: \"#4310FF\",\r\n                      c2: \"#7FA6FF\",\r\n                    },\r\n                  ];\r\n                  return new echarts.graphic.LinearGradient(0, 0, 0, 1, [\r\n                    {\r\n                      offset: 0,\r\n                      color: colorList[params.dataIndex].c1,\r\n                    },\r\n                    {\r\n                      offset: 1,\r\n                      color: colorList[params.dataIndex].c2,\r\n                    },\r\n                  ]);\r\n                },\r\n              },\r\n            },\r\n          },\r\n        ],\r\n      };\r\n      this.chartBar.clear();\r\n      this.chartBar.setOption(option);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}