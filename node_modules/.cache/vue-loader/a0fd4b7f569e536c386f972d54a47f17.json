              {\r\n                        num1:'4.66',\r\n                        num2:'17',\r\n                        left: '0.18',\r\n                        top: '0.79',\r\n\r\n                    },\r\n                    // 鄂尔多斯储气\r\n                    {\r\n                        num1:'9.1',\r\n                        num2:'22',\r\n                        left: '0.18',\r\n                        top: '0.79',\r\n                    },\r\n                    // 包头\r\n                    {\r\n                        num1:'4.66',\r\n                        num2:'22',\r\n                        left: '0.28',\r\n                        top: '0.79',\r\n                    },\r\n                    // 呼和浩特储气\r\n                    {\r\n                        num1:'7.81',\r\n                        num2:'19',\r\n                        left: '0.52',\r\n                        top: '0.91',\r\n                    },\r\n                    // 通辽储气\r\n                    {\r\n                        num1:'0.2',\r\n                        num2:'5',\r\n                        left: '0.62',\r\n                        top: '0.69',\r\n                    },\r\n                ],//储气\r\n                channelList:[\r\n                    // 长庆气田-乌海\r\n                    {\r\n                        name:'长庆气田-乌海-临河输气管道',\r\n                        num1:'355',\r\n                        num2:'6.4',\r\n                        num3:'790',\r\n                        num4:'790',\r\n                        left: '0.17',\r\n                        top: '0.61',\r\n                    },\r\n\r\n                    // 苏-东-淮\r\n                    {\r\n                        name:'苏-东-淮天然气管道',\r\n                         num1:'355',\r\n                        num2:'6.4',\r\n                        num3:'790',\r\n                        num4:'790',\r\n                        left: '0.17',\r\n                        top: '0.61',\r\n                    },\r\n                    {\r\n  